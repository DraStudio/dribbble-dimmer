{
	"manifest_version": 2,

	"name": "Dribbble Dimmer",
	"version": "2.0",
	"description": "Enables the viewing of shots over a dimmed background.",
  	"content_scripts": [ {
    	"css": [ "dribbble-dimmer.css" ],
    	"js": [ "jquery-2.2.2.min.js", "dribbble-dimmer.js" ],
    	"matches": [ "http://www.dribbble.com/*", "http://dribbble.com/*", "https://www.dribbble.com/*", "https://dribbble.com/*" ]
  		}],
  		"icons" : {
			"48" : "icon-48.png",
			"128" : "icon-128.png"
  		},
	"web_accessible_resources": [
		"lightbulb.png"
  	]
}
